    <div class="containerwrapper">
        <div class="contact-form">
            <h3 >REGISTER HERE</h3>
            <form class="register" action="/participants" method="POST">
                <div>
                    <label for="FIRSTNAME">First Name : </label>
                    <input type="text" class="input" placeholder="Enter your First Name" name="fname" required>
                </div>
                <div>
                    <label for="LASTNAME">Last Name : </label>
                    <input type="text" class="input" placeholder="Enter your Last Name" name="lname" required>
                </div>
                <div>
                    <label for="EMAIL">Email ID : </label>
                    <input type="email" class="input" placeholder="Enter your Email ID" name="email" required>
                </div>
                <div>
                    <label for="MALE" class="input">Male</label>
                    <input id="male" type="radio" name="Gender" value="Male" required>
                    <label for="FEMALE" class="input">Female</label>
                    <input id="male" type="radio" name="Gender" value="Female" required>
                </div>
                <div>
                    <select name="stream" id="input" class="sel">
                        <option value="STREAM">STREAM</option>
                        <option value="NONE">NONE</option>
                        <option value="ENGINEERING">ENGINEERING</option>
                        <option value="MEDICAL">MEDICAL</option>
                        <option value="FOUNDATION">FOUNDATION</option>
                       

                    </select>
                    <select name="classes" id="input" class="sel" name="clg">
                        <option value="CLASS">CLASS</option>
                        <option value="NONE">NONE</option>
                        <option value="class VI">CLASS VI</option>
                        <option value="class VII">CLASS VII</option>
                        <option value="CLASS VIII">CLASS VIII</option>
                        <option value="CLASS IX">CLASS IX</option>
                        <option value="CLASS X">CLASS X</option>
                        <option value="CLASS XI">CLASS XI</option>
                        <option value="CLASS XII">CLASS XII</option>
                        
                    </select>
                </div>
                <div>
                    <label for="checkbox">I agree to the terms and conditions</label>
                    <input id="checkbox" type="checkbox" name="checkbox" required="checkbox">
                </div>
                <div class="submit">
                    <input type="submit" class="sub">
                </div>
            </form>
            <div class="side2">
                <img src="images/signup.png" alt="signup img">
            </div>
        </div>
